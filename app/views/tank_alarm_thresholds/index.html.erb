<h1>Listing tank_alarm_thresholds</h1>

<table>
  <thead>
    <tr>
      <th>Tank</th>
      <th>Ph threshold</th>
      <th>Do threshold</th>
      <th>Orp threshold</th>
      <th>K1 threshold</th>
      <th>K10 threshold</th>
      <th>Temp threshold</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @tank_alarm_thresholds.each do |tank_alarm_threshold| %>
      <tr>
        <td><%= tank_alarm_threshold.tank_id %></td>
        <td><%= tank_alarm_threshold.ph_threshold %></td>
        <td><%= tank_alarm_threshold.do_threshold %></td>
        <td><%= tank_alarm_threshold.orp_threshold %></td>
        <td><%= tank_alarm_threshold.k1_threshold %></td>
        <td><%= tank_alarm_threshold.k10_threshold %></td>
        <td><%= tank_alarm_threshold.temp_threshold %></td>
        <td><%= link_to 'Show', tank_alarm_threshold %></td>
        <td><%= link_to 'Edit', edit_tank_alarm_threshold_path(tank_alarm_threshold) %></td>
        <td><%= link_to 'Destroy', tank_alarm_threshold, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tank alarm threshold', new_tank_alarm_threshold_path %>
